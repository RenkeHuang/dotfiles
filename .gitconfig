# git configuration file
[user]
        name = Renke Huang
        email = renke.huang@schrodinger.com
[core]
        excludesfile = ~/.gitignore_global
[push]
        default = tracking
[pull]
        default = current
	rebase = true
[color]
        ui = true
[color "branch"]
        current = yellow reverse
        local = yellow
        remote = green
[color "diff"]
        meta = yellow bold
        frag = magenta bold
        old = red bold
        new = green bold
[color "status"]
        added = yellow
        changed = green
        untracked = cyan
[url "ssh://XXXX/var/git/repos/"]
        # `git clone schro:REPO_NAME`
        insteadOf = schro:
[alias]
        # common typo:       
        satus = status
